<!doctype html>
<html class="no-js" lang="">
<head>
    <meta charset="utf-8">
    <meta name="description" content="">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Torneo de Fútbol Para Socios del Club Atlético San Lorenzo de Almagro</title>
    <link rel="shortcut icon" href="favicon.ico">
    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    <link rel="stylesheet" href="vendor.css">
    <link rel="stylesheet" href="style.css">
    <link rel="stylesheet" type="text/css" href="css/layerslider.css">
    <script src="js/vendor/modernizr.js"></script>
</head>
<body>
<!--[if lt IE 10]>
<p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
<![endif]-->
<div class="wrapper">
    <!-- header -->
    <header class="header-main">
        <div class="header-lower clearfix">
            <%- include ../menu.ejs %>
        </div>
    </header>

    <section class="latestResultTable">
        <div class="container">
            <h2 class="heading2">Fix<span>ture</span></h2>


                <div class="container">
                    <div class="row">
                        <!-- proximo partidos zona 1  -->
                        <% if ((Object.keys(partidos)).length > 0 ){ %>
                        <% for (var i = 0; i < Object.keys(partidos).length;i++ ) {%>
                        <div class="matchSchedule_details row">
                            <% if (i % 2 == 0){ %>
                            <div class="match_next">
                                <div class="wrap_match_next left_triangle ">
                                    <div class="left_padding">
                                        <h4 class="headline03">Fecha<br/><%=(Object.keys(partidos)[i])%></h4>
                                    </div>
                                </div>
                            </div>
                            <% }%>

                            <div class="match_versus-wrap ">
                                <div class="wrap_match-innerdetails">
                                    <ul class="home_tInfo scrollable">
                        <% for (var n = 0; n < (partidos[Object.keys(partidos)[i]]).length;n++ ) {%>
                                        <li>
                                            <a href="#">
                                                <ul class="t_info match_info01 headline01 clearfix">
                                                    <% if (partidos[Object.keys(partidos)[i]][n].estado == "FIN"){ %>
                                                    <li ><%=partidos[Object.keys(partidos)[i]][n].marcador1 %></li>
                                                    <li>
                                                        <div class="headline01 clearfix"><span><%=partidos[Object.keys(partidos)[i]][n].equipo1 %></span><span class="vs"> vs </span><span><%=partidos[Object.keys(partidos)[i]][n].equipo2 %></span></div>
                                                    <li ><%=partidos[Object.keys(partidos)[i]][n].marcador2 %></li>
                                                    <% } else { %>
                                                    <li ><h3><%=partidos[Object.keys(partidos)[i]][n].fecha%></h3></li>
                                                    <li>
                                                        <div class="headline01 clearfix"><span><%=partidos[Object.keys(partidos)[i]][n].equipo1 %></span><span class="vs"> vs </span><span><%=partidos[Object.keys(partidos)[i]][n].equipo2 %></span></div>
                                                    <% }%>
                                                        <div class="paragraph02 clearfix">
                                                            <span></span>
                                                            <span><%=partidos[Object.keys(partidos)[i]][n].cancha %></span>
                                                            <span><%=partidos[Object.keys(partidos)[i]][n].estado %></span>
                                                            <span></span>
                                                        </div>
                                                    </li>
                                                    <li ></li>
                                                </ul>
                                            </a>
                                        </li>


                        <% }%>
                                    </ul>
                                </div>
                            </div>
                            <% if (i % 2 != 0){ %>
                            <div class="match_next">
                                <div class="wrap_match_next left_triangle ">
                                    <div class="left_padding">
                                        <h4 class="headline03">Fecha<br/><%=(Object.keys(partidos)[i])%></h4>
                                    </div>
                                </div>
                            </div>
                            <% }%>
                        </div>
                        <% }%>
                        <% }%>



                        <script src="js/vendor/vendor.js"></script>
                        <script src="js/main.js"></script>
                        <script src="js/admin.js"></script>
                        <script src="js/divisionesTorneo.js"></script>



    </section>
            <!-- footer -->
            <footer ><!--class="footer-type01"-->
                <%- include ../footer.ejs %>
            </footer>

</div>
</body>
</html>


