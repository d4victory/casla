<!doctype html>
<html class="no-js" lang="">
    <head>
        <meta charset="utf-8">
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>Torneo de Fútbol Para Socios del Club Atlético San Lorenzo de Almagro</title>
        <link rel="shortcut icon" href="favicon.ico">
        <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
        <link rel="stylesheet" href="vendor.css">
        <link rel="stylesheet" href="style.css">
        <link rel="stylesheet" type="text/css" href="css/layerslider.css">
        <script src="js/vendor/modernizr.js"></script>    
    </head>
    <body>
        <!--[if lt IE 10]>
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->
        <div class="wrapper">
            <!-- header -->
            <header class="header-main">
                <div class="header-lower clearfix">
                    <%- include ../menu.ejs %>
                </div>
            </header>
            <!-- banner section -->
            <div class="innerbannerwrap">
                <div class="content"></div>
                <div class="innerbannergoleadores">
                    <h2 class="bannerHeadline">Cargar datos del partido<span></span></h2>
                </div>
            </div>
            <% if (resultado) { %>
            <% if (resultado == 200) { %>
            <div class="alert alert-success">La operacion fue realizada con exito</div>
            <% } else { %>
            <div class="alert alert-danger">La operacion fallo. Inténtelo nuevamente más tarde</div>
            <% } %>
            <% } %>
            <form>
                <table style="width:100%">

                <tr>
                  <td>Fecha Numero:</td>
                  <td id="nro-fecha-id"></td>
                  <td>Fecha:</td>
                  <td><input id="fecha-id" type="date" name="fecha"></td>
                </tr>

                <tr>
                  <td>Division: </td>
                  <td id="division-id"></td>
                  <td>Estado:</td>
                  <td><select id="estado-id"></select></td>
                </tr>

                <tr>
                  <th colspan="2" id="team1-id">Equipo1</th>
                  <th colspan="2" id="team2-id">Equipo2</th> 
                </tr>
                <tr>
                  <td>Goles: </td>
                  <td><input type="number" id="goles1-id" name="goles1"></td>
                  <td>Goles: </td>
                  <td><input type="number" id="goles2-id" name="goles2"></td>
                </tr>
                <tr>

                  <td colspan="2">
                    <div class="card">
                      <div class="card-header" role="tab" id="hAmonestados1-id">
                        <h5 class="mb-0">
                          <a class="collapsed" data-toggle="collapse" href="#amonestados1-id" aria-expanded="false" aria-controls="amonestados1-id">
                            Amonestados
                          </a>
                        </h5>
                      </div>
                      <div id="amonestados1-id" class="collapse" role="tabpanel" aria-labelledby="hAmonestados1-id">
                          Jugador1 <br>
                          Jugador1 <br>
                          Jugador1 <br>
                          Jugador1 <br>
                          Jugador1 <br>
                      </div>
                    </div>
                    <div class="card">
                      <div class="card-header" role="tab" id="hExpulsados1-id">
                        <h5 class="mb-0">
                          <a class="collapsed" data-toggle="collapse" href="#expulsados1-id" aria-expanded="false" aria-controls="expulsados1-id">
                            Expulsados
                          </a>
                        </h5>
                      </div>
                      <div id="expulsados1-id" class="collapse" role="tabpanel" aria-labelledby="hExpulsados1-id">
                          Jugador1 <br>
                          Jugador1 <br>
                          Jugador1 <br>
                          Jugador1 <br>
                      </div>
                    </div>
                    <div class="card">
                      <div class="card-header" role="tab" id="hCambios1-id">
                        <h5 class="mb-0">
                          <a class="collapsed" data-toggle="collapse" href="#cambios1-id" aria-expanded="false" aria-controls="cambios1-id">
                            Cambios
                          </a>
                        </h5>
                      </div>
                      <div id="cambios1-id" class="collapse" role="tabpanel" aria-labelledby="hCambios1-id">
                          Jugador1 <br>
                          Jugador1 <br>
                      </div>
                    </div>
                  </td>

                  <td colspan="2">
                    <div class="card">
                      <div class="card-header" role="tab" id="hAmonestados2-id">
                        <h5 class="mb-0">
                          <a class="collapsed" data-toggle="collapse" href="#amonestados2-id" aria-expanded="false" aria-controls="amonestados2-id">
                            Amonestados
                          </a>
                        </h5>
                      </div>
                      <div id="amonestados2-id" class="collapse" role="tabpanel" aria-labelledby="hAmonestados2-id">
                          Jugador1 <br>
                          Jugador1 <br>
                          Jugador1 <br>
                          Jugador1 <br>
                          Jugador1 <br>
                      </div>
                    </div>
                    <div class="card">
                      <div class="card-header" role="tab" id="hExpulsados2-id">
                        <h5 class="mb-0">
                          <a class="collapsed" data-toggle="collapse" href="#expulsados2-id" aria-expanded="false" aria-controls="expulsados2-id">
                            Expulsados
                          </a>
                        </h5>
                      </div>
                      <div id="expulsados2-id" class="collapse" role="tabpanel" aria-labelledby="hExpulsados2-id">
                          Jugador1 <br>
                          Jugador1 <br>
                          Jugador1 <br>
                          Jugador1 <br>
                      </div>
                    </div>
                    <div class="card">
                      <div class="card-header" role="tab" id="hCambios2-id">
                        <h5 class="mb-0">
                          <a class="collapsed" data-toggle="collapse" href="#cambios2-id" aria-expanded="false" aria-controls="cambios2-id">
                            Cambios
                          </a>
                        </h5>
                      </div>
                      <div id="cambios2-id" class="collapse" role="tabpanel" aria-labelledby="hCambios2-id">
                          Jugador1 <br>
                          Jugador1 <br>
                      </div>
                    </div>
                  </td>

                </tr>
                </table>
              </form>
              <button type="submit" class="botonCancelar-id" data-dismiss="modal">Cancelar</button>
              <button type="submit" class="botonGuardarCambios-id">Guardar cambios</button>
            <!-- footer -->
            <footer >
                <!--class="footer-type01"-->
                <%- include ../footer.ejs %>
            </footer>
        </div>

        <script src="https://code.jquery.com/jquery-3.1.1.slim.min.js" integrity="sha384-A7FZj7v+d/sdmMqp/nOQwliLvUsJfDHW+k9Omg/a/EheAdgtzNs3hpfag6Ed950n" crossorigin="anonymous"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/tether/1.4.0/js/tether.min.js" integrity="sha384-DztdAPBWPRXSA/3eYEEUWrWCy7G5KFbe8fFjk5JAIxUYHKkDx6Qin1DkWx51bBrb" crossorigin="anonymous"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-alpha.6/js/bootstrap.min.js" integrity="sha384-vBWWzlZJ8ea9aCX4pEW3rVHjgjt7zpkNpZk+02D9phzyeVkE+jo0ieGizqPLForn" crossorigin="anonymous"></script>
        <script src="js/vendor/vendor.js"></script>     
        <script src="js/main.js"></script>
        <script src="js/admin.js"></script> 
        <script src="js/cargarPartido.js"></script>
        
    </body>
</html>